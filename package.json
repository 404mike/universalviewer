{
  "name": "universalviewer",
  "version": "4.0.0-pre.55",
  "description": "The Universal Viewer is a community-developed open source project on a mission to help you share your content with the world.",
  "engines": {
    "node": ">=10.4.0",
    "npm": ">=3.10.8"
  },
  "files": [
    "dist/",
    "webpack-helpers.js"
  ],
  "main": "dist/uv-dist-umd/UV.js",
  "scripts": {
    "build:dist": "npm run lint:all && grunt build --dist",
    "build": "grunt build",
    "e2eserve": "npx serve dist -p 4444",
    "lint:all": "prettier --write \"./src/**/*.{js,jsx,json,css,ts,tsx}\" \"!./src/lib/* \"",
    "start": "npx webpack serve -c webpack.dev-server.js",
    "build:webpack": "webpack -c webpack.config.js",
    "theme:en": "THEME=uv-en-gb-theme NPM_THEME=@universalviewer/uv-en-gb-theme webpack -c webpack.theme.js",
    "theme:cy": "THEME=uv-cy-gb-theme NPM_THEME=@universalviewer/uv-cy-gb-theme webpack -c webpack.theme.js",
    "test": "jest",
    "prepublishOnly": "npm run build:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/universalviewer/universalviewer.git"
  },
  "keywords": [
    "IIIF",
    "Viewer",
    "Image",
    "AV",
    "PDF",
    "ePub",
    "3D"
  ],
  "author": "@edsilv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/universalviewer/universalviewer/issues"
  },
  "homepage": "https://github.com/universalviewer/universalviewer",
  "devDependencies": {
    "@types/jest": "22.2.2",
    "@types/puppeteer": "1.9.0",
    "@webpack-cli/serve": "1.6.0",
    "async": "0.9.0",
    "chai": "4.1.2",
    "chalk": "0.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "glob": "7.1.3",
    "glob-array": "1.0.0",
    "grunt": "1.0.3",
    "grunt-cli": "1.3.1",
    "grunt-contrib-clean": "0.4.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "0.4.1",
    "grunt-webpack": "^4.0.3",
    "hooker": "0.2.3",
    "html-webpack-plugin": "5.3.2",
    "jest": "23.6.0",
    "jest-puppeteer": "3.5.0",
    "less": "4.1.2",
    "less-loader": "10.1.0",
    "lodash": "4.17.11",
    "ncp": "2.0.0",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "puppeteer": "1.9.0",
    "style-loader": "3.3.0",
    "ts-jest": "22.4.2",
    "ts-loader": "9.2.6",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.4.3",
    "typescript-tslint-plugin": "^0.5.5",
    "url-loader": "4.1.1",
    "webpack": "5.59.1",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.3.1"
  },
  "dependencies": {
    "@edsilv/http-status-codes": "1.0.3",
    "@edsilv/key-codes": "1.0.0",
    "@edsilv/utils": "1.0.2",
    "@google/model-viewer": "1.2.1",
    "@iiif/base-component": "2.0.1",
    "@iiif/iiif-av-component": "^1.1.7",
    "@iiif/iiif-gallery-component": "1.1.19",
    "@iiif/iiif-metadata-component": "^1.1.19",
    "@iiif/iiif-tree-component": "^2.0.3",
    "@iiif/manifold": "2.0.5",
    "@iiif/vocabulary": "^1.0.21",
    "@types/modernizr": "3.2.29",
    "@types/node": "8.10.52",
    "@universalviewer/aleph": "0.0.15",
    "@universalviewer/uv-cy-gb-theme": "1.1.2",
    "@universalviewer/uv-ebook-components": "1.0.2",
    "@universalviewer/uv-en-gb-theme": "1.1.3",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "core-js": "3.8.0",
    "jquery": "3.5.0",
    "jsviews": "0.9.83",
    "manifesto.js": "4.2.4",
    "mediaelement": "4.2.15",
    "mediaelement-plugins": "2.5.1",
    "openseadragon": "2.4.2",
    "pdfjs-dist": "2.0.161",
    "pdfobject": "2.1.1",
    "slideatlas-viewer": "4.4.1",
    "whatwg-fetch": "^3.0.0",
    "xss": "1.0.3"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/universalviewer",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    },
    "preset": "jest-puppeteer",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
